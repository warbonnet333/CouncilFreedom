<div class="bank_block-list_item">
  <div class="toggle_title js-toggle-trigger">
    <div class="toggle_title_name">
      {{name}} ({{ua}})
    </div>
    <div class="toggle_title_icon">
      <svg width="30" height="30" viewBox="0 0 30 30" fill="none" xmlns="http://www.w3.org/2000/svg">
        <rect class="vert" x="13.1992" width="3.33333" height="30" fill="black"/>
        <rect class="horiz" x="30" y="14.3999" width="3.33333" height="30" transform="rotate(90 30 14.3999)"
              fill="black"/>
      </svg>
    </div>
  </div>
  <div class="toggle_text js-toggle-target">{{{html}}}</div>
</div>
